# TAMU OnCHAIN Identity Layer - Environment Configuration
# Fail .env: Untuk simpan rahsia sistem

# Server Configuration
PORT=4000
ORIGIN=http://localhost:3000

# Database Configuration (PostgreSQL)
DATABASE_URL=postgresql://postgres:password@db:5432/tamu_identity

# RPC Configuration (Ethereum/Blockchain)
RPC_URL=https://ethereum-rpc.publicnode.com

# Nonce Configuration
NONCE_TTL_MIN=5

# ENS Configuration
REQUIRE_REVERSE_ENS=false

# Webhook Security
WEBHOOK_SECRET=your-webhook-secret-change-this

# Rate Limiting (Phase 3A)
RATE_LIMIT_ENABLED=true
REDIS_URL=redis://redis:6379
RATE_LIMIT_CACHE_TTL_SEC=60
